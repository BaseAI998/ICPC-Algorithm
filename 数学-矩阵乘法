const int mod = 1e9+7;
const int N = 107;
struct MAT {
    int v[N][N];
    MAT() {
        memset(v, 0, sizeof(v));
    }
};
int n;
MAT operator * (const MAT &a, const MAT &b) {
    MAT c;
    for(int k=0;k<n;++k) {
        for(int i=0;i<n;++i) {
            for(int j=0;j<n;++j) {
                c.v[i][j] = (c.v[i][j] + a.v[i][k]*b.v[k][j]%mod) % mod;
            }
        }
    }
    return c;
}
