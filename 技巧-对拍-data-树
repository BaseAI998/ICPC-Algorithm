#include<bits/stdc++.h>
using namespace std;
int rd(int L,int R) {
    return rand()%(R-L+1) + L;
}
int main()
{
    srand((unsigned)time(0));
    int T = 3;
    printf("%d\n",T);
    while(T--) {
        int n = 10;
        printf("%d\n",n);
        for(int i=2;i<=n;++i) {
            int x = rd(1, i-1);
            printf("%d %d\n",i, x);
        }
    }
    return 0;
}
